#include <stdio.h>

int main() {
    int n;
    //printf("Nhap so phan tu cua day: ");
    scanf("%d", &n);

    int A[n];
    //printf("Nhap cac phan tu cua day:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &A[i]);
    }

    //printf("Day sau khi dao nguoc:\n");
    for (int i = n - 1; i >= 0; i--) {
        printf("%d ", A[i]);
    }

    return 0;
}
