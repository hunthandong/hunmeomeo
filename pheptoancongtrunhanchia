#include "stdio.h"
#include "string.h"
int main(){
float x,y;
char pt;
scanf("%f %f %c",&x,&y,&pt);
switch(pt){
case '+':{
printf("%g",(x+y));
break;
}
case '-':{
printf("%g",(x-y));
break;
}
case '*':{
printf("%g",(x*y));
break;
}
case '/':{
if(y==0){
printf("error");
}
else{
printf("%g",(x/y));
}
break;
}
default:{
printf("error");
break;
}
return 0;
}
}
