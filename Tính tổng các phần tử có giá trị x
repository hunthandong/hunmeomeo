#include <stdio.h>

int main() {
    int n, x;
    //printf("Nhap do dai day so: ");
    scanf("%d", &n);
    //printf("Nhap gia tri x: ");
    scanf("%d", &x);

    int A[n];
    //printf("Nhap cac phan tu cua day: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &A[i]);
    }

    int sum = 0;
    for (int i = 0; i < n; i++) {
        if (A[i] == x) {
            sum += A[i];
        }
    }

    printf("%d\n", sum);

    return 0;
}

