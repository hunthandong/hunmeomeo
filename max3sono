#include"stdio.h"
int main (){
	int a,b,c ;
//	printf (" nhap vao 2 so a b c :");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b && a>c){
	//	printf("so lon nhat la:");
		printf("%d",a);
	}
	else if (b>a && b>c)
	{
		//printf ("so lon nhat la:");
		printf("%d",b);
		}
	else if (c>b && c>a)
	{
	//	printf("so lon nhat la:");
		printf("%d",c);
		}
	return 0;
}
